---
import { Image } from 'astro:assets';
import Layout from '../../layouts/Layout.astro';

import img1 from './tst1.jpg';
import img2 from './tst2.jpg';
import Gallery from '../../components/Gallery.astro';
---

<Layout title="hi">
  <Gallery
    defaultWidth={100}
    images={[
      {
        image: img1,
        altText: 'Image1',
      },
      {
        image: img2,
        altText: 'Image2',
      },
    ]}
  />
</Layout>
