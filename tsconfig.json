{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@libs/*": [
        "src/libs/*",
      ],
      "@components/*": [
        "src/components/*",
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
  },
}